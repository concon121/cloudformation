<% priority = 1 %>
  <%= @id %>:
    Type: "AWS::WAF::WebACL"
    Properties:
      Name: <%= @name %>
      DefaultAction:
        Type: "ALLOW"
      MetricName: <%= @name %>
      Rules: <% @rules.each do |ruleId| %>
        - Action:
            Type: "BLOCK"
          Priority: <%= priority %> <% priority = priority + 1 %>
          RuleId:
            Ref: <%= ruleId %> <% end %>
